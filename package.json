{
  "name": "dp-table-builder-ui",
  "version": "1.1.1",
  "homepage": "https://www.npmjs.com/package/dp-table-builder-ui",
  "description": "Handsontable grid implementation",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "build": "babel src -d dist && npm run csscopy",
    "csscopy": "copyfiles -u 1 src/**/*.scss dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./src/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "ons-digital-publishing",
  "contributors": [
    "Mas Ahmad <masahmad7@gmail.com>"
  ],
  "repository": "github:ONSdigital/dp-table-builder-ui",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "testURL": "http://localhost/",
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "modulePathIgnorePatterns": [
      "test-app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    },
    "globals": {
      "__HOT_BUILD_DATE__": "",
      "__HOT_PACKAGE_NAME__": "",
      "__HOT_VERSION__": "",
      "__HOT_BASE_VERSION__": ""
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cross-env": "5.1.2",
    "file-saver": "^1.3.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-handsontable": "^0.3.1",
    "react-hot-loader": "4.0.0-beta.1",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-es2015": "7.0.0-beta.53",
    "@babel/preset-react": "^7.18.6",
    "babel-eslint": "8.1.2",
    "babel-jest": "29.1.2",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "11.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.25.5",
    "css-loader": "6.7.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "node-sass": "^8.0.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "3.0.2",
    "sass-loader": "13.1.0",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.74.0",
    "webpack-dev-server": "4.11.1"
  }
}
