{
  "name": "dp-table-builder-ui",
  "version": "1.1.2",
  "homepage": "https://www.npmjs.com/package/dp-table-builder-ui",
  "description": "Handsontable grid implementation",
  "main": "dist/index.js",
  "scripts": {
    "audit": "npx --yes auditjs ossi --whitelist ./audit-allowlist.json --quiet",
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "build": "babel src -d dist && npm run csscopy",
    "csscopy": "copyfiles -u 1 src/**/*.scss dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile",
    "lint": "eslint ./src/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "ons-digital-publishing",
  "contributors": [
    "Mas Ahmad <masahmad7@gmail.com>"
  ],
  "repository": "github:ONSdigital/dp-table-builder-ui",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    },
    "globals": {
      "__HOT_BUILD_DATE__": "",
      "__HOT_PACKAGE_NAME__": "",
      "__HOT_VERSION__": "",
      "__HOT_BASE_VERSION__": ""
    }
  },
  "dependencies": {
    "@handsontable/react": "^15.3.0",
    "cross-env": "7.0.3",
    "file-saver": "^1.3.3",
    "handsontable": "^15.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.0.0-beta.1",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-eslint": "8.1.2",
    "babel-jest": "29.7.0",
    "babel-loader": "10.0.0",
    "babel-polyfill": "^6.26.0",
    "copyfiles": "^1.0.0",
    "core-js": "^2.5.3",
    "css-loader": "7.1.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "mini-css-extract-plugin": "2.9.2",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "6.0.1",
    "sass": "^1.89.0",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.1"
  }
}
